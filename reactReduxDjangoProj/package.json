{
  "name": "react.redux.django.proj",
  "version": "1.0.0",
  "description": "The boilerplate for React Redux Django SPA",
  "main": "index.js",
  "scripts": {
    "build-prod": "webpack --config webpack.config.prod.babel.js --progress --colors",
    "build": "webpack --config webpack.config.dev.babel.js --progress --colors",
    "build:watch": "webpack --config webpack.config.dev.babel.js --watch",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "esw webpack.configt.* tools assets/src",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"assets/src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist"
  },
  "author": "Feng Shang",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "colors": "^1.1.2",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "expect": "^1.20.2",
    "file-loader": "^0.11.2",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.4.2",
    "mocha-jsdom": "^1.1.0",
    "nock": "^9.0.13",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-cookies": "^0.1.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-thunk": "^1.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fshang26/reactReduxDjangoEnv.git"
  }
}
